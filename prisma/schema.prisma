generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id        Int      @id @default(autoincrement())
  isbn      String
  title     String
  thumbnail String?
  addressId String
  createdAt DateTime @default(now())
  authors   Author[]
}

model Author {
  id    Int    @id @default(autoincrement())
  name  String @unique
  books Book[]
}
